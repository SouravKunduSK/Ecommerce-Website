<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="" />
    <meta name="author" content="" />
    <link rel="shortcut icon" href="~/Content/images/favicon.ico" type="image/x-icon">
    <title>Shopping - Seller</title>
    <link href="https://cdn.jsdelivr.net/npm/simple-datatables@latest/dist/style.css" rel="stylesheet" />

    <link href="~/AdminStyle/css/styles.css" rel="stylesheet" />
    <script src="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.3/js/all.min.js" crossorigin="anonymous"></script>
    @Styles.Render("~/Content/css")
</head>
<body class="sb-nav-fixed">
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <!-- Navbar Brand-->

        <a class="navbar-brand ps-3" href="@Url.Action("Index","Dashboard")">@*<img height="80" width="80" src="~/Content/images/logo.png" class="logo" alt="">*@Shopping / Seller</a>
        <!-- Sidebar Toggle-->
        <button class="btn btn-link btn-sm order-1 order-lg-0 me-4 me-lg-0" id="sidebarToggle" href="#!"><i class="fas fa-bars"></i></button>
        <!-- Navbar Search-->
        <form class="d-none d-md-inline-block form-inline ms-auto me-0 me-md-3 my-2 my-md-0">
            <div class="input-group">
                <input class="form-control" type="text" placeholder="Search for..." aria-label="Search for..." aria-describedby="btnNavbarSearch" />
                <button class="btn btn-primary" id="btnNavbarSearch" type="button"><i class="fas fa-search"></i></button>
            </div>
        </form>
        <!-- Navbar-->
        <ul class="navbar-nav ms-auto ms-md-0 me-3 me-lg-4">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="navbarDropdown" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <ul class="dropdown-menu dropdown-menu-end" aria-labelledby="navbarDropdown">




                    <li> <a class="dropdown-item" href="#">Seller Name: @Session["sellername"] </a></li>
                    <li> <a class="dropdown-item" href="@(Session["sellername"] != null ? Url.Action("SaIndex", "Account") : Url.Action("Login", "Account"))" type="button">My Profile</a></li>
                    <li><hr class="dropdown-divider" /></li>
                    <li><a class="dropdown-item" href="@Url.Action("Signout","Account")">Logout</a></li>
                </ul>
            </li>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        @*<div class="sb-sidenav-menu-heading">Core</div>*@
                        <a class="nav-link" href="@Url.Action("Index","Dashboard")">
                            <div class="sb-nav-link-icon"><i class="fas fa-tachometer-alt"></i></div>
                            Dashboard
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","District")">
                            <div class="sb-nav-link-icon"><i class="fas fa-pen"></i></div>
                            Districts
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Categories")">
                            <div class="sb-nav-link-icon"><i class="fas fa-calendar-check"></i></div>
                            Categories
                        </a>
                        <a class="nav-link" href="@Url.Action("Index","Product")">
                            <div class="sb-nav-link-icon"><i class="fas fa-store"></i></div>
                            Products
                        </a>

                        <a class="nav-link" href="@Url.Action("Index","Order")">
                            <div class="sb-nav-link-icon"><i class="fas fa-pen"></i></div>
                            Orders
                        </a>

                        <a class="nav-link" href="@Url.Action("Index","User")">
                            <div class="sb-nav-link-icon"><i class="fas fa-user"></i></div>
                            Customers
                        </a>

                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div class="small">Logged in as:</div>
                    @Session["sellername"]
                </div>
            </nav>
        </div>

        <div id="layoutSidenav_content">
            <!-- Icon Cards-->
            @RenderBody()




            <!-- /.container-fluid -->
            <!-- Sticky Footer -->

            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid px-4">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">
                            Copyright &copy;
                            <script>document.write(new Date().getFullYear());</script> All rights reserved  by Shopping.<i class="icon-heart color-danger" aria-hidden="true"></i>
                        </div>
                        <div>
                            <a href="#">Privacy Policy</a>
                            &middot;
                            <a href="#">Terms &amp; Conditions</a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/AdminStyle/js/scripts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>
    <script src="~/AdminStyle/assets/demo/chart-area-demo.js"></script>
    <script src="~/AdminStyle/assets/demo/chart-bar-demo.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/simple-datatables@latest" crossorigin="anonymous"></script>
    <script src="~/AdminStyle/js/datatables-simple-demo.js"></script>


</body>

</html>
@Scripts.Render("~/Content/js")
@RenderSection("scripts", required: false)

